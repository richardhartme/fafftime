{
  "name": "fafftime-analyser",
  "version": "1.0.0",
  "description": "Ultra Cycling Fafftime Analyser - Find where you spent time stopped instead of riding",
  "main": "src/main.ts",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "serve": "webpack serve --mode development --open",
    "start": "npm run serve",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "deploy": "./deploy.sh staging",
    "deploy:staging": "./deploy.sh staging",
    "deploy:production": "./deploy.sh production",
    "build:deploy": "npm run build && npm run deploy:staging",
    "build:deploy:production": "npm run build && npm run deploy:production"
  },
  "keywords": [
    "cycling",
    "garmin",
    "fit",
    "analysis",
    "ultra-cycling"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@garmin/fitsdk": "^21.178.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^24.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "jsdom": "^26.1.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
